    <Paragraph Type="Dialogue">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Prime" RevisionID="0" Size="12" Style="">Ok! Ok, I won't come in.</Text>
      <Text> </Text>
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Prime" RevisionID="0" Size="12" Style="">I'm staying right outside your door though. I want to be the first person you see when you come out.</Text>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
    </Paragraph>
    <Paragraph Type="Character">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Prime" RevisionID="0" Size="12" Style="AllCaps">RUTH</Text>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
