    <Paragraph Type="Dialogue">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Prime" RevisionID="0" Size="12" Style="">WHY THE FUCK AM I LIKE THIS?</Text>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
    </Paragraph>
    <Paragraph Type="Action">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Prime" RevisionID="0" Size="12" Style="">Ruth drops her head to her knees. She tries to control her breathing, but can't.</Text>
      <UserParagraphData>
                
