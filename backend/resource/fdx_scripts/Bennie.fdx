    </Paragraph>
    <Paragraph Type="Dialogue">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Prime" RevisionID="0" Size="12" Style="">Mom?</Text>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
