    <Paragraph Type="Dialogue">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Prime" RevisionID="0" Size="12" Style="">No! No-no, don't!</Text>
      <Text> </Text>
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Prime" RevisionID="0" Size="12" Style="">Don't come in! You-you can't--</Text>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
