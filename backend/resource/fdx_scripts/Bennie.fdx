    </Paragraph>
    <Paragraph Type="Dialogue">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Prime" RevisionID="0" Size="12" Style="">Do you want me to come in?</Text>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
    </Paragraph>
    <Paragraph Type="Character">
      <Text AdornmentStyle="0" Background="#FFFFFFFFFFFF" Color="#000000000000" Font="Courier Prime" RevisionID="0" Size="12" Style="AllCaps">RUTH</Text>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
            </UserParagraphData>
      <UserParagraphData>
                
